<!DOCTYPE html>
<html>
  <head>
    <title>Gallery</title>
  </head>

  <body>
    <header>
      <h1>Gallery</h1>
    </header>

    <main>
      <% if (gifs[0] !== 'error') { %>
        <% gifs.forEach(function(gif) { %>
            <img src=<%= gif %>>
        <% }); %>
      <% } else { %>
        <p>Error getting results</p>
      <% } %>
    </main>
  </body>
</html>